{
    "version": 2,
    "name": "todo-backend",
    "builds": [{ "src": "server.js", "use": "@vercel/node" }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js",
            "headers": {
                "Access-Control-Allow-Origin": "https://todo-frontend-ujjwaljamuar.vercel.app/login",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE"
            }
        }
    ]
}